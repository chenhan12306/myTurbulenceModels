FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel        kOmegaSSTLMRough;

    turbulence      on;

    printCoeffs     on;

    roughnessModel  Kozulovic2022;   // Kozulovic2022, Langel2017, 

    maxLambdaIter  100;
}

